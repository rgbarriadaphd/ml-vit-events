{
    "dataset": {
        "path": "input/vit_features_clinical_baseline.csv",
        "transform": {
            "standardization": false,
            "normalization": false,
            "outliers": false,
            "collinear": false,
            "relevant": false
        }
    },
    "settings": ["ALL","NO RD","RD"],
    "parameters": {
        "test_size": 0.3,
        "n_folds": 5,
        "random_state": 42
    },
    "classifiers": {
        "logistic_regression": {
            "penalty": "l2",
            "C": 1.0,
            "solver": "liblinear"
        },
        "random_forest": {
            "n_estimators": 100,
            "max_depth": 5,
            "random_state": 42
        },
        "svc": {
            "kernel": "linear",
            "C": 1.0
        }
    }
}